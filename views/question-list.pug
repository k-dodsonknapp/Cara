extends home.pug

include utils.pug

block content
  div(class='py-3')
    a(class='btn btn-success' href='/question/add' role='button') Add Question
  table(class='table table-striped table-hover')
    thead(class='thead-dark')
      tr
        th(scope='col') Questions
    tbody
      each question in questions
        tr
          td= question
